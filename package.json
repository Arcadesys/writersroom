{
  "name": "writersroom",
  "version": "2.0.5",
  "description": "Obsidian plugin to call ChatGPT prompts by prompt ID and highlight edits.",
  "main": "main.js",
  "scripts": {
    "dev": "esbuild --bundle main.ts --outfile=main.js --external:obsidian --external:@codemirror/* --format=cjs --platform=node --target=es2020 --watch",
    "build": "esbuild --bundle main.ts --outfile=main.js --external:obsidian --external:@codemirror/* --format=cjs --platform=node --target=es2020",
    "deploy": "npm run build && cp main.js '/Users/austen/Documents/Writing Projects/.obsidian/plugins/writersroom/main.js'",
    "test": "vitest run"
  },
  "keywords": [
    "obsidian-plugin",
    "chatgpt"
  ],
  "author": "Austen",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.11.0",
    "esbuild": "^0.21.5",
    "obsidian": "^1.5.8",
    "typescript": "^5.4.0",
    "vitest": "^1.6.0"
  },
  "dependencies": {
    "openai": "^6.5.0"
  }
}
